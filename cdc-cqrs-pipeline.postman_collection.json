		{
			"name": "Create-MySQL-Debezium-Connector",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Accept",
						"value": "application/json"
					},
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"name\": \"app-mysql-db-connector\",\r\n    \"config\": {\r\n        \"connector.class\": \"io.debezium.connector.mysql.MySqlConnector\",\r\n        \"tasks.max\": \"1\",\r\n        \"database.hostname\": \"mysql_db_server\",\r\n        \"database.port\": \"3306\",\r\n        \"database.user\": \"custom_mysql_user\",\r\n        \"database.password\": \"custom_mysql_user_password\",\r\n        \"database.server.id\": \"184054\",\r\n        \"database.server.name\": \"app-mysql-server\",\r\n\t\t\"database.whitelist\": \"app-mysql-db\",\r\n        \"database.history.kafka.bootstrap.servers\": \"kafka_server:29092\",\r\n        \"database.history.kafka.topic\": \"dbhistory.app-mysql-db\",\r\n\t\t\"include.schema.changes\": \"true\"\r\n    }\r\n}"
				},
				"url": {
					"raw": "localhost:8083/connectors/",
					"host": [
						"localhost"
					],
					"port": "8083",
					"path": [
